{
  "name": "sofia-plugin",
  "version": "4.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "node tests/runAll.js",
    "build:openapi": "node ./scripts/generate_openapi.js",
    "prepare:render": "node ./scripts/prepare_render.js",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "typecheck": "tsc --noEmit",
    "pre-deploy": "node scripts/pre_deploy_check.js",
    "setup:hooks": "bash scripts/setup_git_hooks.sh",
    "health": "curl -s http://localhost:10000/health | json_pp",
    "health:detailed": "curl -s http://localhost:10000/health/detailed | json_pp"
  },
  "dependencies": {
    "@octokit/rest": "^20.1.2",
    "ajv": "^8.12.0",
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "simple-git": "^3.19.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.30.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
